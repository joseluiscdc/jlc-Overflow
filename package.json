{
  "name": "jlc-overflow",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -r \"npm run start:server\" \"npm run start:client\"",
    "start:server": "NODE_ENV=development DEBUG=platzi-overflow* nodemon server/index.js",
    "start:client": "ng serve",
    "start:prod": "NODE_ENV=production node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm run build:client && npm run build:server",
    "build:client": "ng build --configuration=production",
    "build:server": "babel server --out-dir dist/server",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.7",
    "@angular/cdk": "^10.1.2",
    "@angular/cli": "^10.0.5",
